{
  "name": "@projectedby/cms",
  "version": "0.0.12",
  "description": "ProjectedBy Content Management System",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "build:theme": "npm run build --workspace theme",
    "build:cms": "npx projectedby static gen --theme=theme/cms --destination=docs",
    "build": "npm run build:theme && npm run build:cms",
    "test": "mocha --recursive",
    "test:static": "npm run test --workspace static"
  },
  "bin": {
    "projectedby": "./src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/projectedby/cms.git"
  },
  "keywords": [
    "ProjectedBy",
    "CMS",
    "NodeJS",
    "Javascript",
    "Novemberizing"
  ],
  "author": "novemberizing <novemberizing@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/projectedby/cms/issues"
  },
  "homepage": "https://github.com/projectedby/cms#readme",
  "workspaces": [
    "static",
    "theme",
    "exception",
    "script"
  ],
  "dependencies": {
    "ejs": "^3.1.9",
    "minimist": "^1.2.8",
    "showdown": "^2.1.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "bootstrap": "^5.3.2"
  }
}
